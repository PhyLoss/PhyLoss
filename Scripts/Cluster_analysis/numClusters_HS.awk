# get taxID from geneID (id)
function getTaxId(id) {
	#id == "pgi|0000000003120175935|ti|312017|pi|0|" ---> taxid=312017
	pos = index(id, "ti|");
	s = substr(id, pos + 3); # skip "ti|"
	# find next "|", which ends taxid
	pos2 = index(s, "|");
	taxid = substr(s, 1, pos2 - 1);
    return taxid
}

